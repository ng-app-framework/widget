{
  "name": "@ng-app-framework/widget",
  "version": "1.1.5",
  "license": "MIT",
  "keywords": [
    "ng-app",
    "ng-app-framework"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --watch=false",
    "lint": "ng lint",
    "package": "ng-packagr && cp -r ./public/ ./dist/public",
    "patch": "npm version patch --force && ng-packagr && cp -r ./public/ ./dist/public && npm publish dist",
    "minor": "npm version minor --force && ng-packagr && cp -r ./public/ ./dist/public && npm publish dist",
    "major": "npm version major --force && ng-packagr && cp -r ./public/ ./dist/public && npm publish dist"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^5.1.0",
    "@angular/core": "^5.1.0",
    "bootstrap": "^4.0.0-beta.2",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "popper.js": "^1.13.0",
    "rxjs": "^5.5.5",
    "tether": "^1.4.3"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.0",
    "@angular/compiler": "^5.1.0",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@types/jasmine": "2.5.45",
    "@types/node": "^6.0.93",
    "ajv": "^5.5.1",
    "codelyzer": "^4.0.2",
    "core-js": "^2.5.3",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "https://registry.npmjs.org/karma-chrome-launcher/-/karma-chrome-launcher-2.0.0.tgz",
    "karma-cli": "^1.0.1",
    "karma-commonjs": "^1.0.0",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.6.0",
    "karma-spec-reporter": "0.0.26",
    "ng-packagr": "^1.6.0",
    "node-sass": "^4.3.0",
    "ts-node": "^1.2.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "zone.js": "^0.8.18"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
